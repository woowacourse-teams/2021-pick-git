<?xml version="1.0" encoding="UTF-8"?>
<included>
  <appender name="test-basic-logger" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <charset>utf8</charset>
      <Pattern>
        %d{yyyy-MM-dd HH:mm:ss.SSS} %-5level %t %class{36}.%M L:%L %n      > %m%n
      </Pattern>
    </encoder>
  </appender>
</included>
